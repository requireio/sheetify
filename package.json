{
  "name": "sheetify",
  "version": "4.0.5",
  "description": "Modular CSS bundler",
  "repository": "sheetify/sheetify",
  "license": "MIT",
  "scripts": {
    "deps": "dependency-check . --entry transform.js . && dependency-check . --entry transform.js --extra --no-dev -i insert-css",
    "test": "standard && npm run deps && tape test/*",
    "test:cov": "standard && npm run deps && NODE_ENV=test istanbul cover test/coverage.js",
    "format": "standard --format"
  },
  "browser": {
    "index.js": "./browser.js"
  },
  "keywords": [
    "modular",
    "css",
    "bundle",
    "browserify",
    "css-modules"
  ],
  "dependencies": {
    "end-of-stream": "^1.1.0",
    "falafel": "^1.2.0",
    "insert-css": "^0.2.0",
    "is-stream": "^1.0.1",
    "map-limit": "0.0.1",
    "mkdirp": "^0.5.1",
    "postcss": "^5.0.10",
    "postcss-prefix": "^1.0.3",
    "resolve": "^1.1.7",
    "style-resolve": "0.0.0",
    "through2": "^2.0.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "concat-stream": "^1.5.1",
    "css-type-base": "^1.0.2",
    "dependency-check": "^2.5.1",
    "istanbul": "^0.3.19",
    "jsdom": "^8.0.2",
    "npm-check-updates": "^2.2.0",
    "rimraf": "^2.5.1",
    "sheetify-cssnext": "^1.0.0",
    "standard": "^5.2.1",
    "tape": "^4.2.0"
  }
}
